<h2><%= @user.nickname %>'s Account Balance </h2>
<%= link_to "収入登録", new_income_path %>
<%= link_to "支出登録", new_outgo_path %>

<table>
  <tr>
    <th>日付</th>
    <th>収入</th>
    <th>金額</th>
    <th>収支</th>
  </tr>
  <tr>
    <% @incomes.each do |income| %>
    <td><%= income.date %></td>
    <td><%= income.name %></td>
    <td><%= income.price %></td>
    <% end %>
  </tr>


</table>
